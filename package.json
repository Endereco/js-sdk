    "@eslint/js": "^9.21.0",
    "axios": "^1.8.1",
    "babel-loader": "^10.0.0",
    "babel-preset-stage-3": "^7.0.0-beta.3",
    "browser-sync": "^3.0.3",
    "classlist-polyfill": "^1.2.0",
    "copyfiles": "^2.4.1",
    "cors": "^2.8.5",
    "cross-env": "^7.0.2",
    "css-loader": "^7.1.2",
    "custom-event-polyfill": "^1.0.7",
    "diff": "^7.0.0",
    "eslint": "^8.57.1",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-promise": "^6.6.0",
    "express": "^4.21.2",
    "fast-levenshtein": "^3.0.0",
    "globals": "^15.0.0",
    "html-loader": "^5.1.0",
    "lodash.isequal": "^4.5.0",
    "lodash.merge": "^4.6.2",
    "mini-css-extract-plugin": "^2.9.2",
    "mustache": "^4.2.0",
    "nodemon": "^3.1.9",
    "promise-polyfill": "^8.3.0",
    "rimraf": "^6.0.1",
    "sass": "^1.85.1",
    "sass-loader": "^16.0.5",
    "terser-webpack-plugin": "^5.3.12",
    "uuid": "^11.1.0",
    "webpack": "^5.98.0",
    "webpack-cli": "^6.0.1"
  },
