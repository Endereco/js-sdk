{
  "name": "@endereco/js-sdk",
  "version": "1.11.0",
  "main": "index.js",
  "scripts": {
    "build": "npm run build-styles && cross-env NODE_ENV=production NODE_OPTIONS=--openssl-legacy-provider webpack --progress && npm run copy",
    "build-styles": "cross-env NODE_ENV=production NODE_OPTIONS=--openssl-legacy-provider webpack --config webpack-styles.config.js && rimraf dist/style.js && npm run copy-styles",
    "copy": "copyfiles -u 1 ./dist/endereco.min.js ./demo/assets && copyfiles -u 1 ./dist/endereco.min.js.map ./demo/assets",
    "copy-styles": "copyfiles -u 1 ./dist/endereco.min.css ./demo/assets",
    "demo": "nodemon -e js,njk,mjs,cjs,json,html demo/app.js"
  },
